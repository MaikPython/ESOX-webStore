{       
    "env":{  
        "browser": true,
        "node": true
    },
    "settings":{
        "react": {
            "version": "detect"
        }     
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module"
    },

    "rules": {
        "quotes": ["error", "double"]
    },
 
    "extends":[
        "eslint:recommended",
        "plugin:react/recommended"
    ]

}